{
	"parser": "@typescript-eslint/parser",
	"extends": ["plugin:@typescript-eslint/recommended", "plugin:jest/recommended", "plugin:prettier/recommended"],
	"plugins": ["filenames"],
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"project": "tsconfig.json"
	},
	"rules": {
		"filenames/match-regex": [2, "^[0-9a-z-.]+$", true]
	}
}
